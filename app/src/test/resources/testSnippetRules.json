{
  "rules": [
    {
      "description": "snippets - cx -> chmodx",
      "keys": "cx",
      "replacement": "chmod +x"
    },
    {
      "description": "snippets - ij -> intellij",
      "keys": "cx",
      "replacement": "intellij"
    },
    {
      "description": "snippets - fb -> foo@bar.com",
      "keys": "cx",
      "replacement": "foo@bar.com"
    }
  ]
}
